{
  "$schema": "../workload_schema.json",
  "sections": [
    {
      "groups": [
        {
          "inserts": {
            "op_count": 1000000,
            "key": {
              "segmented": {
                "separator": "",
                "segments": [
                  "user",
                  { "uniform": { "len": 20, "character_set": "numeric" } },
                  "object",
                  { "uniform": { "len": 20, "character_set": "numeric" } }
                ]
              }
            },
            "val": {
              "uniform": { "len": 1024 }
            }
          }
        },
        {
          "point_queries": {
            "op_count": 500000,
            "selection": {
              "beta": {
                "alpha": 1,
                "beta": 5
              }
            }
          },
          "empty_point_queries": {
            "op_count": 50000,
            "key": {
              "segmented": {
                "separator": "",
                "segments": [
                  "user",
                  { "uniform": { "len": 20, "character_set": "numeric" } },
                  "object",
                  { "uniform": { "len": 20, "character_set": "numeric" } }
                ]
              }
            },
            "selection": {
              "beta": {
                "alpha": 1,
                "beta": 5
              }
            }
          },
          "range_queries": {
            "op_count": 5000,
            "selectivity": { "normal": { "mean": 0.01, "std_dev": 0.005 } }
          },
          "updates": {
            "op_count": 500000,
            "val": {
              "uniform": { "len": 128 }
            },
            "selection": {
              "beta": {
                "alpha": 2,
                "beta": 3
              }
            }
          }
        }
      ]
    }
  ]
}
